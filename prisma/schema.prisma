generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./chess.db"
}

model checkmate {
  game String?
  id   Int     @id @default(autoincrement())
}

model initial {
  moves    String? @unique(map: "initial_moves_uindex")
  progress String? @unique(map: "sqlite_autoindex_initial_1")
  length   Int?    @default(4)
  time     Int?    @default(0)
  id       Int     @id @default(autoincrement())
}
